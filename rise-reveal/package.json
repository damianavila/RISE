{
  "name": "rise-reveal",
  "description": "original reveal.js patched for use within the RISE Jupyter extension",
  "license": "MIT",
  "keywords": [
    "reveal.js",
    "RISE",
    "Jupyter",
    "slideshow",
    "slides"
  ],
  "author": {
    "name": "Dami√°n Avila",
    "url": "https://github.com/damianavila"
  },
  "contributors": [
    {
      "name": "Thierry Parmentelat",
      "url": "https://github.com/parmentelat"
    }
  ],
  "homepage": "https://github.com/damianavila/RISE/tree/rise-reveal/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/damianavila/RISE"
  },
  "bugs": {
    "url": "https://github.com/damianavila/RISE/issues"
  },
  "files": [
    "export/reveal.js",
    "export/reveal.js-chalkboard"
  ],
  "scripts": {
    "build": "bash -c 'for target in copy patch; do npm run $target; done'",
    "copy": "bash -c 'mkdir -p export; cp -r ./node_modules/reveal.js/ ./export/reveal.js/; cp -r ./node_modules/reveal.js-plugins/chalkboard ./export/reveal.js-chalkboard/'",
    "patch": "bash -c 'for target in patch-reveal-css patch-notes patch-themes patch-chalkboard ; do npm run $target; done'",
    "patch-reveal-css": "bash patch-reveal-css.sh",
    "patch-notes": "bash patch-notes-plugin.sh",
    "patch-themes": "bash patch-reveal-themes.sh",
    "patch-chalkboard": "bash patch-chalkboard.sh",
    "clean": "bash -c 'rm -rf node_modules export'"
  },
  "dependencies": {
    "reveal.js": "~3.9.2",
    "reveal.js-plugins": "rajgoel/reveal.js-plugins#3.9.0"
  },
  "version": "390.0.1"
}
